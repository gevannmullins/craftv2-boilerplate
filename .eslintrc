{
  "env": {
    "shared-node-browser": true,
    "browser": true,
    "node": true,
    "jquery": true,
    "worker": true,
    "es6": true
  },
  "globals": {
    "_": false
  },
  "extends": "airbnb",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "rules": {
    "array-bracket-spacing": [ "error", "always", { "singleValue": true, "objectsInArrays": false, "arraysInArrays": false } ],
    "arrow-parens": [ "error", "always" ],
    "brace-style": [ "error", "1tbs", { "allowSingleLine": false } ],
    "class-methods-use-this": "off",
    "comma-dangle": [ "error", "never" ],
    "consistent-return": "off",
    "curly": [ "error", "all" ],
    "func-call-spacing": [ "error", "never" ],
    "func-names": "off",
    "global-require": "off",
    "import/extensions": "off",
    "import/imports-first": "off",
    "import/no-dynamic-require": "off",
    "import/no-extraneous-dependencies": "off",
    "import/no-unresolved": "off",
    "max-len": "off",
    "new-cap": "warn",
    "no-console": [ "warn", { "allow": [ "info", "warn", "debug", "error" ] }],
    "no-mixed-operators": "off",
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 1 }],
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-prototype-builtins": "off",
    "no-restricted-syntax": [ "error", "WithStatement" ],
    "no-return-assign": "off",
    "no-spaced-func": "error",
    "no-template-curly-in-string": "off",
    "no-underscore-dangle": "off",
    "no-unneeded-ternary": "off",
    "no-unused-vars": [ "warn", { "argsIgnorePattern": "req|res|next|err|reject", "args": "after-used" } ],
    "no-warning-comments": "off",
    "prefer-const": "warn",
    "quote-props": [ "warn", "as-needed" ],
    "react/require-extension": "off",
    "space-before-function-paren": [ "error", "never" ],
    "strict": [ "error", "never" ]
  }
}
